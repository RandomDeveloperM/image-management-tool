{
  "name": "image-management-tool",
  "version": "0.0.1",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "install-app-deps",
    "build-electron": "ng build --base-href . && xcopy main.js dist\\main.js* && xcopy package.json dist\\package.json* && xcopy server dist\\server /E /S /I",
    "electron": "npm run build-electron && electron dist",
    "electron-win": "npm run build-electron && cd dist && npm install && cd .. && electron-packager ./dist ImageManagementTool --out ./build --platform=win32 --arch=x64 --overwrite"
  },
  "private": true,
  "dependencies": {
